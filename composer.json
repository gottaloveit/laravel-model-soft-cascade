{
    "name": "touhidurabir/laravel-model-soft-cascade",
    "description": "A laravel package to handle cascade delete and restore on model relations.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Touhidurabir\\ModelSoftCascade\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Touhidurabir\\ModelSoftCascade\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Touhidur Rahman",
            "email": "abircse06@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.21",
        "illuminate/support": "^8.62",
        "illuminate/database": "^8.62",
        "illuminate/events": "^8.62"
    },
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Touhidurabir\\ModelSoftCascade\\ModelSoftCascadeServiceProvider"
            ],
            "aliases": {
                "ModelUuid": "Touhidurabir\\ModelSoftCascade\\Facades\\ModelSoftCascade"
            }
        }
    }
}
